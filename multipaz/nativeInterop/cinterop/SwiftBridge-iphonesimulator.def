package = org.multipaz
language = Objective-C

headers = SwiftBridge/SwiftBridge-Swift.h
headerFilter = SwiftBridge/*

staticLibraries = libSwiftBridge.a
libraryPaths = SwiftBridge/build/Release-iphonesimulator

linkerOpts = -L/usr/lib/swift
linkerOpts.ios_x64 = -platform_version ios-simulator 14.0 14.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
linkerOpts.ios_arm64 = -platform_version ios-simulator 14.0 14.0 -L/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/swift/iphonesimulator/
